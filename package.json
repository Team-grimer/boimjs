  "scripts": {
    "test": "jest --forceExit --runInBand --detectOpenHandles",
    "build:asset": "webpack --config ./config/webpack.asset.config.js",
    "build:lib": "webpack --config ./config/webpack.lib.config.js",
    "build:hydration": "webpack --config ./config/webpack.hydration.config.js",
    "build:dynamic": "webpack --config ./config/webpack.dynamic.config.js",
    "build:prod": "webpack --config ./config/webpack.prod.config.js",
    "build": "npm run build:lib && npm run build:asset && npm run build:hydration && npm run build:dynamic && npm run build:prod",
    "start": "NODE_ENV=production npm run build && node ../../dist/server/_www",
    "build:dev": "webpack --config ./config/webpack.dev.config.js",
    "dev": "NODE_ENV=development npm run build:lib && NODE_ENV=development npm run build:asset && NODE_ENV=development npm run build:hydration && NODE_ENV=development npm run build:dynamic && NODE_ENV=development npm run build:dev && NODE_ENV=development node ./server/_dev"
  },
