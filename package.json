{
  "name": "boim",
  "version": "1.0.2",
  "description": "The React Framework",
  "keywords": [
    "server side rendering",
    "nextjs",
    "react",
    "react ssr",
    "react ssg"
  ],
  "main": "main.js",
  "homepage": "https://github.com/Team-grimer/boimjs",
