{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../../dist/compile/",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "es6",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
  },
  "include": [
    "*"
  ],
}